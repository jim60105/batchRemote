{
  "manifest_version": 2,

  "name": "batchRemote",
  "description": "batchRemote",
  "version": "0.1",
  "author": "jim60105",
  "background": { "scripts": ["background.js"] },
  "content_scripts":[{
    "matches":["http://batchremote.jim60105.com/batch.html"],
    "js":["jquery-2.2.0.min.js","content_script.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "alarms"
  ]
}
