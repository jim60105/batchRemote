{
  "manifest_version": 2,

  "name": "batchRemote",
  "description": "batchRemote",
  "version": "0.1",
  "author": "jim60105",
  "background": { "scripts": ["background.js"] },
  "content_scripts":[{
    "matches":["http://webbatch.vghtc.gov.tw/*"],
    "js":["content_script.js"]
  }],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "alarms"
  ]
}
